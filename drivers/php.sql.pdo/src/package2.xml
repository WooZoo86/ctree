    <min>7.0.11</min>
   </php>
   <pearinstaller>
    <min>1.4.0</min>
   </pearinstaller>
   <package>
    <name>pdo</name>
    <channel>pecl.php.net</channel>
    <min>1.0.3</min>
    <providesextension>PDO</providesextension>
